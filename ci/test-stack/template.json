{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
    "TestQueueName": {
      "Type": "String",
      "Default" : "aws-cloudformation-stack-test-default"
    }
  },
  "Resources": {
    "SqsQueue": {
      "Type": "AWS::SQS::Queue",
      "Properties": {
        "DelaySeconds": 10,
        "MaximumMessageSize": 1024,
        "MessageRetentionPeriod": 60,
        "QueueName": {
          "Ref": "TestQueueName"
        },
        "VisibilityTimeout": 30
      }
    }
  }
}